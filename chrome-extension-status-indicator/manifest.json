{
  "manifest_version": 3,
  "name": "予約状況インジケーター",
  "version": "1.0.0",
  "description": "ふじみ野ひかり眼科の一般予約・視野予約の状況をツールバーに表示",
  "permissions": [
    "alarms",
    "storage"
  ],
  "host_permissions": [
    "https://docs.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "予約状況を確認"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
